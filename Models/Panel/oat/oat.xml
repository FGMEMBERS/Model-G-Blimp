<?xml version="1.0"?>
  <!-- outside air temp -->
  <PropertyList>
 
    <path>oat.ac</path>

    <animation> 
      <type>rotate</type>
      <object-name>needle</object-name>
      <property>environment/temperature-degc</property>
      <interpolation>
        <entry> 
          <ind>-40</ind>
          <dep>-30</dep>
        </entry>
        <entry>
          <ind>-30</ind>
          <dep>-24</dep>
        </entry>
        <entry> 
          <ind>-20</ind>
          <dep>-18</dep>
        </entry>
        <entry> 
          <ind>-10</ind>
          <dep>-10</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>-4.5</dep>
        </entry>
        <entry> 
          <ind>10</ind>
          <dep>0</dep>
        </entry>
        <entry>
        <entry> 
          <ind>20</ind>
          <dep>4.5</dep>
        </entry> 
          <ind>25</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>30</ind>
          <dep>15</dep>
        </entry>
        <entry> 
          <ind>35</ind>
          <dep>17</dep>
        </entry>
        <entry> 
          <ind>40</ind>
          <dep>25</dep>
        </entry>
      </interpolation>
      <center>
        <x-m>0.00</x-m>
        <y-m>0</y-m>
        <z-m>-0.022</z-m>
      </center>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

    <animation>
      <type>material</type>
      <emission>
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.7</red>
        <green>0.70</green>
        <blue>0.70</blue>
      </emission>
    </animation>

    <animation>
		<type>noshadow</type>
    </animation>
    <!-- *** LoD *** -->
    <animation>
 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>6</max-m>
    </animation>
  </PropertyList>
