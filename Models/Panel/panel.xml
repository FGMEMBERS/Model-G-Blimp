<PropertyList>

 <path>panel.ac</path>

 <offsets>
  <!-- x/y/z == back/right/up -->
  <x-m> 0.0 </x-m>
  <y-m> 0.0 </y-m>
  <z-m> 0.0 </z-m>
 </offsets>

 <!-- 3d instruments -->

  <model>
  <name>Clock</name>
  <path>Aircraft/Model-G-Blimp/Models/Panel/compass/compass.xml</path>
  <offsets>
   <heading-deg>90</heading-deg>
   <pitch-deg>0</pitch-deg>
   <x-m>  0.170 </x-m>
   <y-m>  -0.240 </y-m>
   <z-m>  -0.102 </z-m>
  </offsets>
 </model>

 <model>
  <name>vertical-speed</name>
  <path>Aircraft/Model-G-Blimp/Models/Panel/VSI/VSI.xml</path>
  <offsets>
   <heading-deg>0</heading-deg>
   <pitch-deg>-24</pitch-deg>
   <x-m>  0.059 </x-m>
   <y-m>  -0.190 </y-m>
   <z-m>  -0.0670 </z-m>
  </offsets>
 </model>

 <model>
  <name>Altimeter</name>
  <path>Aircraft/Model-G-Blimp/Models/Panel/alt/alt.xml</path>
