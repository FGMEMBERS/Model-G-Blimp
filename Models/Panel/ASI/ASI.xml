<?xml version="1.0"?>

<PropertyList>

 <path>ASI.ac</path>

<effect>
     <inherits-from>Effects/model-transparent</inherits-from>
          <object-name>Glass</object-name>
       </effect>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PointerTip</object-name>
  <emission>
   <red>0.62</red>
   <green>0.62</green>
   <blue>0.62</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>PointerTip</object-name>
  <object-name>PointerCentre</object-name>
  <property>velocities/airspeed-kt</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>4.34</ind><dep>40.2</dep></entry>
   <entry><ind>8.68</ind><dep>84</dep></entry>
   <entry><ind>13.034</ind><dep>117</dep></entry>
   <entry><ind>17.38</ind><dep>153</dep></entry>
   <entry><ind>21.72</ind><dep>193</dep></entry>
   <entry><ind>26.09</ind><dep>230</dep></entry>
   <entry><ind>30.41</ind><dep>270</dep></entry>
   <entry><ind>34.75</ind><dep>307</dep></entry>
   <entry><ind>39.104</ind><dep>342</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>

</PropertyList>
